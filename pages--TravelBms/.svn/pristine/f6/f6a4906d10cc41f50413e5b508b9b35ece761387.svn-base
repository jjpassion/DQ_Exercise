.content_bigbox{width:1179px;float:left;overflow:hidden;}
.search{overflow:hidden;margin-left:180px;}
.search input{outline:none;border:solid 1px #40abdf;width:68px;height:14px;background-color:transparent;float:left;margin:20px 0 20px 10px;}
.search .rizhileixing{width:120px;}
.search a{float:right;margin-top:18px;margin-right:300px;}
.search p{float:left;margin:20px 0 20px 20px;line-height:18px;}
.search select{float:left;outline:none;border:solid 1px #40abdf;width:60px;height:18px;margin:20px 0 20px 10px;}
.taitou{width:1154px;margin-left:10px;background-image:url(images/biaodan_01.png);height:43px;font-size:16px;font-family:微软雅黑;line-height:40px;text-align:center;}
.jiewei{margin-left:10px;}

.biaodan{overflow:hidden;}
.taitou_02{background-image:url(images/biaodan_02.png);background-repeat:no-repeat;margin-left:10px;width:100%;overflow:hidden;}
.taitou_02 dd{float:left;line-height:36px;color:white;font-size:14px;font-family:微软雅黑;}
.taitou_02 input{float:left;margin-left:26px;margin-top:12px;}
.xtrz_leibie_02{width:23%; text-align:center;}
.xtrz_leibie_03{width:24.5%; text-align:center;}
.xtrz_leibie_04{width:23.5%;text-align:center;}
.xtrz_leibie_05{width:23%;text-align:center;}
.content_01{background-image:url(images/biaodan_bg_gray.png);background-repeat:no-repeat;margin-left:10px;width:1154px;height:36px;}
.content_01 dd,.content_02 dd{float:left;line-height:36px;height:36px;}
.content_01 input,.content_02 input{float:left;margin-left:26px;margin-top:12px;}
.xtrz_name{width:24%;text-align:center;}
.xtrz_sex{width:24%;text-align:center;}
.xtrz_account{width:24%;text-align:center;}
.xtrz_sfzh{width:24%;text-align:center;}

.content_02{background-image:url(images/biaodan_bg_white.png);background-repeat:no-repeat;margin-left:10px;width:1154px;height:36px;}
.content_btn{margin-top:40px;overflow:hidden;float:left;}
.content_btn a{float:left;margin-left:20px;background-color:#40ABEF;width:86px;height:36px;color:white;font-size:14px;font-family:微软雅黑;line-height:36px;text-align:center;}
.content_btn .dc{width:100px;}
.content_btn .dc img{float:left;padding-top:8px;padding-left:8px;}



.search .Wdate{
	border:#40abdf 1px solid;
	width:140px;
	background-image:url(images/datePicker.gif);background-repeat:no-repeat;background-position:right;
}
.Wdate::-ms-clear{display:none;}

.WdateFmtErr{
	font-weight:bold;
	color:red;
}
