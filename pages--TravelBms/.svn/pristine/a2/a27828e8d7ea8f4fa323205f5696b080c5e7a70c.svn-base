s).find('.embed-container').empty();								  var id=	$(this).attr("id");								  if(id=="popup-distribution"){										//分配										$(".ms2side__div").remove();																			}								},								open:function(){									var id=	$(this).attr("id");																			if(id=="popup-distribution"){										//分配																				showMultiSelect();									}							    }							});							 							  $(document).on('click', '[data-action="watch-add"]', function(e){							 								e.preventDefault();							 								var plugin = $('#popup-dialog.popup').data('popup');							 								plugin.open();							  });							  							  $(document).on('click', '[data-action="watch-batch"]', function(e){							 								e.preventDefault();							 								var plugin = $('#popup-batch.popup').data('popup');							 								plugin.open();							  });							  							  $(document).on('click', '[data-action="watch-distribution"]', function(e){							 								e.preventDefault();							 								var plugin = $('#popup-distribution.popup').data('popup');							 								plugin.open();							  });							  							  $(document).on('click', '[data-action="watch-delete"]', function(e){							 								e.preventDefault();							 								var plugin = $('#popup-delete.popup').data('popup');							 								plugin.open();							  });								  							   $(document).on('click', '[data-action="watch-xq"]', function(e){							 								e.preventDefault();							 								var plugin = $('#popup-xq.popup').data('popup');							 								plugin.open();							  });								  							   $(document).on('click', '[data-action="watch-bj"]', function(e){							 								e.preventDefault();							 								var plugin = $('#popup-bj.popup').data('popup');							 								plugin.open();							  });								  }  </script>